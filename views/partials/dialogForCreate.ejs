<dialog class="dialogForCreateFolder" >
    <h2>New folder</h2>
    <form action="/newFolder/<%=locals.currentFolder%>/<%=locals.currentSubfolder || 'none'%>" method="POST">
      <input type="text" name="newFolder" id="newFolder" maxlength = "15" required />
      <div class="buttonForCreateFolder">
          <button type="submit">Create</button>
          <button type="button" class="cancelButtonForCreate">Cancel</button>
      </div>
    </form>
</dialog>